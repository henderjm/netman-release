{
  "name": "cni-wrapper",
  "type": "cni-wrapper-plugin",
  "cniVersion": "0.2.0",
  "datastore": "<%= p('cni-flannel.cni_datastore_path') %>",
  "delegate": {
    "name": "cni-flannel",
    "type": "flannel",
    "subnetFile": "<%= p('cni-flannel.flannel.subnet_file') %>",
    "stateDir": "<%= p('cni-flannel.flannel.state_dir') %>",
    "delegate": {
      "bridge": "cni-flannel0",
      "isDefaultGateway": true,
      "ipMasq": false
     }
  }
}
